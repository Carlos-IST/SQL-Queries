UPDATE SA_GROUP_TABLE INNER JOIN SIN_GROUP_TABLE 

ON (SA_GROUP_TABLE.COBERTURA = SIN_GROUP_TABLE.COBERTURA) AND 
   (SA_GROUP_TABLE.SUBTIPO_SEGURO = SIN_GROUP_TABLE.SUBTIPO_SEGURO) AND 
   (SA_GROUP_TABLE.TIPO_BIEN = SIN_GROUP_TABLE.TIPO_BIEN) AND 
   (SA_GROUP_TABLE.TIPO_PRIMER_RIESGO = SIN_GROUP_TABLE.TIPO_PRIMER_RIESGO) AND 
   (SA_GROUP_TABLE.TIPO_CARTERA = SIN_GROUP_TABLE.TIPO_CARTERA) AND 
   (SA_GROUP_TABLE.PRIMERA_LINEA_MAR = SIN_GROUP_TABLE.PRIMERA_LINEA_MAR) AND 
   (SA_GROUP_TABLE.ENTIDAD = SIN_GROUP_TABLE.ENTIDAD) AND 
   (SA_GROUP_TABLE.USO = SIN_GROUP_TABLE.USO) AND 
   (SA_GROUP_TABLE.FORMA_VENTA = SIN_GROUP_TABLE.FORMA_VENTA) AND 
   (SA_GROUP_TABLE.MONEDA = SIN_GROUP_TABLE.MONEDA) 
   
SET SA_GROUP_TABLE.NUMERO_SINIESTROS = SIN_GROUP_TABLE.NUM_SIN, 
    SA_GROUP_TABLE.MONTO_SINIESTRO = SIN_GROUP_TABLE.MONTO_SIN, 
    SA_GROUP_TABLE.GASTOS_AJUSTE = SIN_GROUP_TABLE.GA, 
    SA_GROUP_TABLE.SALVAMENTOS = SIN_GROUP_TABLE.SALV, 
    SA_GROUP_TABLE.MONTO_PAGADO = SIN_GROUP_TABLE.MONTO_PAG, 
    SA_GROUP_TABLE.MONTO_DEDUCIBLE = SIN_GROUP_TABLE.MONTO_DED, 
    SA_GROUP_TABLE.MONTO_COASEGURO = SIN_GROUP_TABLE.MONTO_COA;
